{
    "manifest_version": 3,
    "name": "Github Sidebar Resizer",
    "description": "Change github sidebar width",
    "version": "1.0",
    "icons": {
      "16": "icons/menu.png",
      "48": "icons/menu.png",
      "128": "icons/menu.png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Resize Sidebar"
    },
    "content_scripts": [
      {
        "matches": ["https://github.paypal.com/*"],
        "css": ["style.css"]
      }
    ]
  }
